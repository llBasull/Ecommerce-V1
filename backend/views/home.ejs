<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Bootstrap demo</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/remixicon/4.2.0/remixicon.css"
    />
    <link rel="stylesheet" href="/home.css" />
  </head>
  <body>
    <!-- NAVBAR -->
    <div class="nav_container">
      <nav class="navbar">
        <h1>SYI STUDIO</h1>
        <div class="menu-toggle" id="mobile-menu">
          <span class="bar"></span>
          <span class="bar"></span>
          <span class="bar"></span>
        </div>
        <ul class="nav no-search">
          <li class="nav-item"><a href="#">Home</a></li>
          <li class="nav-item"><a href="#">About</a></li>
          <li class="nav-item"><a href="/products">Products</a></li>
          <li class="nav-item"><a href="/product">Product</a></li>
          <li class="nav-item"><a href="#">Contact Us</a></li>
        </ul>
      </nav>
    </div>
    <!-- ========= -->
    <!-- HERO SECTION -->
    <div class="hero_container">
      <div class="cutout-div">
        <div class="arrow">
          <img
            src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645be3411cec2dfbf913eb86_arrow.svg"
            alt=""
            style="width: 50%; cursor: pointer"
          />
        </div>
      </div>
      <div class="hero-content">
        <h1 style="color: #fff">
          Indulge in your ultimate <br />
          passion with our latest <br />
          arrivals collection
        </h1>
        <button class="btn">Join the movement</button>
      </div>
    </div>

    <!-- trending section -->
    <div class="trending_container">
      <div class="trending-title">
        <h1>Trending</h1>
        <button class="viewMore-btn">
          <p>View more</p>
          <img
            src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645be3411cec2dfbf913eb86_arrow.svg"
            alt=""
            style="width: 50%; cursor: pointer"
          />
        </div>
        </button>
      </div>

      <div class="trending-container">
        <div class="catogary">
          <div class="cato-image">
            <img src="/645e27a2a712b0144adc27fa_image 860.jpg" alt="" />
          </div>
          <div class="name">New arrivals</div>
        </div>
        <div class="catogary">
          <div class="cato-image">
            <img src="/645e27a2a712b0144adc27fa_image 860.jpg" alt="" />
          </div>
          <div class="name">New arrivals</div>
        </div>
        <div class="catogary">
          <div class="cato-image">
            <img src="/645e27a2a712b0144adc27fa_image 860.jpg" alt="" />
          </div>
          <div class="name">New arrivals</div>
        </div>
        <div class="catogary">
          <div class="cato-image">
            <img src="/645e27a2a712b0144adc27fa_image 860.jpg" alt="" />
          </div>
          <div class="name">New arrivals</div>
        </div>
      </div>
    </div>

    <!-- product range section -->
    <div class="category-section">
      <div class="category">
        <div class="left">
          <div class="outerware">
            <img
              src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645d2eba3523924c6fdb0f6b_reynier-carl-87m1_NfKld4-unsplash-p-500.webp"
              alt=""
            />
          </div>
          <div class="shoes">
            <img
              src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645d2eba3523924c6fdb0f6b_reynier-carl-87m1_NfKld4-unsplash-p-500.webp"
              alt=""
            />
          </div>
        </div>
        <div class="right">
          <div class="outerware">
            <img
              src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645d2eba3523924c6fdb0f6b_reynier-carl-87m1_NfKld4-unsplash-p-500.webp"
              alt=""
            />
          </div>
          <div class="outerware">
            <img
              src="https://assets-global.website-files.com/645a69e5ff9a553155774bec/645d2eba3523924c6fdb0f6b_reynier-carl-87m1_NfKld4-unsplash-p-500.webp"
              alt=""
            />
          </div>
        </div>
      </div>
    </div>

    <!-- marquee section -->

    <!-- selection section -->
    <div class="selection_container">
      <h1 style="padding: 0">Check out selection</h1>
      <section id="filters">
        <button class="selectedFilter">New arrivals</button>
        <button>Bestsellers</button>
        <button>Swimwear</button>
        <button>Denim Collection</button>
      </section>
      <section id="products">
        <!-- Products will be rendered dynamically here -->
      </section>
    </div>

    <!-- weekend ad section -->
    <div class="ad_container"></div>

    <!-- JAVASCRIPT -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="/home.js"></script>
  </body>
</html>
